<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.web.WebView?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10"
          stylesheets="@style.css" fx:id="gridPane">
    <Button GridPane.rowIndex="0" GridPane.columnIndex="0" text="click..." onAction="#handleClick"/>
    <Button GridPane.rowIndex="0" GridPane.columnIndex="1" text="button 2"/>
    <Button GridPane.rowIndex="0" GridPane.columnIndex="2" text="button 3" id="button3"/>
    <Button GridPane.rowIndex="0" GridPane.columnIndex="3" text="button 4" fx:id="button4"/>
    <RadioButton text="choose one" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <Hyperlink text="www.javafx.com" onAction="#handleLinClick" GridPane.columnIndex="0" GridPane.rowIndex="3"/>

    <WebView fx:id="webView" GridPane.columnIndex="0" GridPane.rowIndex="4" GridPane.columnSpan="5" GridPane.rowSpan="3" />

    <Label text="JavaFx Effects" fx:id="label" GridPane.columnIndex="1" GridPane.rowIndex="1" rotate="90"
    onMouseEntered="#handleMouseEnter" onMouseExited="#handleMouseExit"/>
</GridPane>